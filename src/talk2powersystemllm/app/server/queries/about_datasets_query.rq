PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?uri ?name ?date {
    ?uri a dcat:Dataset
    OPTIONAL {
        ?uri dct:description ?name.
        FILTER(lang(?name) != "no")
    }
    OPTIONAL {
        ?uri dct:title ?name.
    }
    OPTIONAL {
        SELECT ?uri (SAMPLE(SUBSTR(STR(?dateTime), 1, 10)) AS ?date) {
            ?uri a dcat:Dataset;
            dct:issued ?dateTime
        }
        GROUP BY ?uri
    }
}
ORDER BY ?name
